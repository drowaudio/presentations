tangle":{"y":1130.41015625,"x":137,"width":645.3984375,"height":19}},{"text":"    \/** Should return the properties of the node.","targetRectangle":{"y":1168.41015625,"x":137,"width":472.0078125,"height":19}},{"text":"        This should not be called until after initialise.","targetRectangle":{"y":1187.41015625,"x":137,"width":549.0703125,"height":19}},{"text":"    *\/","targetRectangle":{"y":1206.41015625,"x":137,"width":57.796875,"height":19}},{"text":"    virtual NodeProperties getNodeProperties() = 0;","targetRectangle":{"y":1225.41015625,"x":137,"width":491.2734375,"height":19}},{"text":"    \/** Should return true when this node is ready to be processed.","targetRectangle":{"y":1263.41015625,"x":137,"width":645.3984375,"height":19}},{"text":"        This is usually when its input's output buffers are ready.","targetRectangle":{"y":1282.41015625,"x":137,"width":635.765625,"height":19}},{"text":"    *\/","targetRectangle":{"y":1301.41015625,"x":137,"width":57.796875,"height":19}},{"text":"    virtual bool isReadyToProcess() = 0;","targetRectangle":{"y":1320.41015625,"x":137,"width":385.3125,"height":19}},{"text":"    ","targetRectangle":{"y":1339.41015625,"x":137,"width":38.53125,"height":19}},{"text":"    \/** Struct to describe a single iteration of a process call. *\/","targetRectangle":{"y":1358.41015625,"x":137,"width":645.3984375,"height":19}},{"text":"    struct ProcessContext","targetRectangle":{"y":1377.41015625,"x":137,"width":240.8203125,"height":19}},{"text":"    {","targetRectangle":{"y":1396.41015625,"x":137,"width":48.1640625,"height":19}},{"text":"        juce::Range<int64_t> referenceSampleRange;","targetRectangle":{"y":1415.41015625,"x":137,"width":481.640625,"height":19}},{"text":"        AudioAndMidiBuffer buffers;","targetRectangle":{"y":1434.41015625,"x":137,"width":337.1484375,"height":19}},{"text":"    };","targetRectangle":{"y":1453.41015625,"x":137,"width":57.796875,"height":19}},{"text":"    ","targetRectangle":{"y":1472.41015625,"x":